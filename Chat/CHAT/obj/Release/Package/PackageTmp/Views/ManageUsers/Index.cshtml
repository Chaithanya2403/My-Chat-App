@model IEnumerable<BussinessLayer.ManageUser>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/HomePageLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Username)
        </th>
